; If your script is named start-vnc.sh, change the command path accordingly.
[program:vnc]
command=/usr/local/bin/startup-vnc.sh
user=carbon
environment=VNC_USER="carbon",VNC_DISPLAY=":1",VNC_GEOMETRY="1920x1080",VNC_DEPTH="24",VNC_PASS="Carbon123#",VNC_LOCALHOST="no",VNC_SECURITY_TYPES="VncAuth",VNC_PORT="5901",NOVNC_PORT="6080",VNC_DESKTOP="cinnamon"
autostart=true
autorestart=true
startsecs=3
stdout_logfile=/var/log/vnc.out.log
stderr_logfile=/var/log/vnc.err.log
stopsignal=TERM
stopasgroup=true
killasgroup=true
